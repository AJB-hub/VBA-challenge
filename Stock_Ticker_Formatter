Sub Stock_Ticker_WB():

    For Each ws In Worksheets
        Dim LastRow As Variant
        Dim i as Integer
        Dim dataTableRow As Integer
        Dim Ticker As String
        Dim tickerCount as Integer
        Dim openPrice As Double
        Dim closePrice As Double

        'Formatting
        ws.Cells(1, 9).Value = "Ticker"
        ws.Cells(1, 10).Value = "Yearly Change"
        ws.Cells(1, 11).Value = "Percent Change"
        ws.Cells(1, 12).Value = "Total Stock Volume"
        ws.Columns("I:L").EntireColumn.AutoFit


        'Data insert
        LastRow = ws.Cells(Rows.Count, 1).End(xlUp).Row
        dataTableRow = 2

        
        For i = 2 To LastRow

            If ws.Cells(i + 1, 1).Value <> ws.Cells(i, 1).Value Then 'if the next cell ticker is different
                'implement a change

                ws.Cells(dataTableRow, 9).Value = ws.Cells(i, 1).Value
                openPrice = 
                closePrice = ws.Cells(i,6).Value
                ws.Cells(dataTableRow, 10).Value = openPrice - closePrice





                dataTableRow = dataTableRow + 1
            Else

            End If

        Next i



    
    Next ws

End Sub
